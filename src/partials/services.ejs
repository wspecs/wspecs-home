<div id="services">
  <div class="container">
    <h2>Design Essentials</h2>
    <% for(var i=0; i<home.services.length; i+=3) {%>
    <div class="row">
      
      <div class="col-1-3">
        <div>
          <h4><i class="icon-<%- home.services[i].icon  %>"></i></h4>
        </div>
        <div>
          <h4><%- home.services[i].title  %></h4>
          <p><%- home.services[i].content  %></p>
        </div>
      </div>

      <div class="col-1-3">
        <div>
          <h4><i class="icon-<%- home.services[i + 1].icon  %>"></i></h4>
        </div>
        <div>
          <h4><%- home.services[i + 1].title  %></h4>
          <p><%- home.services[i + 1].content  %></p>
        </div>
      </div>

      <div class="col-1-3">
        <div>
          <h4><i class="icon-<%- home.services[i + 2].icon  %>"></i></h4>
        </div>
        <div>
          <h4><%- home.services[i + 2].title  %></h4>
          <p><%- home.services[i + 2].content  %></p>
        </div>
      </div>
    </div>
    <% } %>
  </div>
</div>